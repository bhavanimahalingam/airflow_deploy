node{
   stage('SCM Checkout'){
     git 'https://github.com/bhavanimahalingam/airflow_deploy/'
     }
   stage('Deploy to airflow'){
    sshagent(['airflow_tom']) {
    sh 'scp -o StrictHostKeyChecking=no /var/lib/jenkins/workspace/airflow_bavani/*.py  ec2-user@18.205.106.20:/home/ec2-user/airflow/dags/'
    sh "pwd"
    sh "whoami"
    dir("/home/ec2-user/airflow") {
      sh "airflow webserver -p 8080 & airflow scheduler && fg"
      sh "pwd"
     }
    }
    }
}  
