node{
   stage('SCM Checkout'){
     git 'https://github.com/bhavanimahalingam/airflow_deploy/'
     }
   stage('Deploy to airflow'){
    sshagent(['airflow_tom']) {
    sh 'scp -o StrictHostKeyChecking=no /var/lib/jenkins/workspace/airflow_bavani/*.py  ec2-user@52.5.254.171:/home/ec2-user/airflow/dags/'
    sh "pwd"
    sh "whoami"
    dir("ec2-user@52.5.254.171: /home/ec2-user/airflow") {
      sh "airflow scheduler & airflow webserver -p 8080 && fg"
      sh "pwd"
     }
    }
    }
}  
